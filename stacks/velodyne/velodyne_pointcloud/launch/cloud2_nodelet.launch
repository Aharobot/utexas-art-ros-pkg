<!-- -*- mode: XML -*- -->
<!-- run velodyne_pointcloud/Cloud2Nodelet in a nodelet manager

     arg: calibration = path to calibration file

     $Id$
  -->

<launch>
  <arg name="calibration" default="" />
  <node pkg="nodelet" type="nodelet" name="cloud2_nodelet"
        args="load velodyne_pointcloud/Cloud2Nodelet velodyne_nodelet_manager">
    <param name="angles" value="$(arg calibration)"/>
  </node>
</launch>
