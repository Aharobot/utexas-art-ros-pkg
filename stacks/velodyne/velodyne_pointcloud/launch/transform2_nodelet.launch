<!-- -*- mode: XML -*- -->
<!-- run velodyne_pointcloud/Transform2 in a nodelet manager

     arg: calibration = path to calibration file

     $Id$
  -->

<launch>
  <arg name="calibration" default="" />
  <node pkg="nodelet" type="nodelet" name="transform2_nodelet"
        args="load velodyne_pointcloud/Transform2 velodyne_nodelet_manager" >
    <param name="angles" value="$(arg calibration)"/>
  </node>
</launch>
