<!-- -*- mode: XML -*- -->

<!-- launch file for viewing a compressed camera1394 stream.  -->

<launch>

  <!-- republish with jpeg compression -->
  <node pkg="image_transport" type="republish" name="image_compression"
        args="raw compressed">
    <remap from="in" to="/camera1394/image"/>
    <remap from="out" to="/camera1394/image"/>
  </node>

  <!-- view compressed images -->
  <node pkg="image_view" type="image_view" name="image_view">
    <remap from="image" to="/camera1394/image"/>
    <param name="image_transport" value="compressed"/>
  </node>

</launch>
