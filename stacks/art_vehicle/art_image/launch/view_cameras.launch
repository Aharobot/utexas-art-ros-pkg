<!-- -*- mode: XML -*- -->

<!-- view two front camera images -->

<launch>

  <node pkg="image_view" type="image_view" name="left_front_view"
        args="_image_transport:=compressed">
    <remap from="image" to="/left_front/camera/image_raw/compressed"/>
  </node>

  <node pkg="image_view" type="image_view" name="right_front_view">
    <remap from="image" to="/right_front/camera/image_raw/compressed"/>
    <param name="~/image_transport" value="compressed"/>
  </node>

</launch>
